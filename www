local lib = loadstring(game:HttpGet("https://raw.githubusercontent.com/dawid-scripts/UI-Libs/main/Vape.txt"))()

local win = lib:Window("Football Fusion 2", Color3.fromRGB(44, 120, 224), Enum.KeyCode.RightControl)

-- Magnets Tab
local magnetTab = win:Tab("Magnets")
magnetTab:Toggle("Mags", false, function(state)
    _G.MagEnabled = state
    while _G.MagEnabled do
        -- Magnet functionality implementation
        wait()
    end
end)

magnetTab:Slider("Mag Range", 0, 100, 30, function(value)
    _G.MagRange = value
end)

-- QB Aimbot Tab
local aimbotTab = win:Tab("QB Aimbot")
aimbotTab:Toggle("QB Aimbot", false, function(state)
    _G.AimbotEnabled = state
    while _G.AimbotEnabled do
        -- Aimbot functionality implementation
        wait()
    end
end)

-- Movement Tab
local movementTab = win:Tab("Movement")
movementTab:Toggle("Speed Hack", false, function(state)
    _G.SpeedEnabled = state
    while _G.SpeedEnabled do
        if game.Players.LocalPlayer.Character then
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = _G.SpeedValue
        end
        wait()
    end
end)

movementTab:Slider("Speed Value", 16, 150, 16, function(value)
    _G.SpeedValue = value
end)

movementTab:Toggle("Jump Power", false, function(state)
    _G.JumpEnabled = state
    while _G.JumpEnabled do
        if game.Players.LocalPlayer.Character then
            game.Players.LocalPlayer.Character.Humanoid.JumpPower = _G.JumpValue
        end
        wait()
    end
end)

movementTab:Slider("Jump Power Value", 50, 200, 50, function(value)
    _G.JumpValue = value
end)
